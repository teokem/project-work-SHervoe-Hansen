tolerance 2.0
filetype pdb
output HEWL_POP.pdb

structure 1hel_fixed.pqr       # Lysozym
        number 1
        fixed 0. 0. 0. 0. 0. 0.
        centerofmass
end structure

structure CHL.pdb              # Chloride
        number 11
        inside sphere 0. 0. 0. 50.
end structure

structure POP.pdb              # Tetravalent pyrophosphate
        number 150
        inside sphere 0. 0. 0. 50.
end structure

structure SOD.pdb              # Sodium
        number 600
        inside sphere 0. 0. 0. 50.
end structure
